    <bean id="record" name="r1,r2 r3;r4" class="di.NewlecRecord"
    p:kor="50" p:eng="60" p:math="70">

    </bean>

    <bean id="view" class="di.NewlecRecordView">
        <property name="record" ref="record"/>
    </bean>

    <bean id="cal" class="aop.NewlecCalculator"></bean>
    <bean id="logPrintAroundAdvice" class="aop.LogPrintAroundAdvice"></bean>

    <bean id="proxy" class="org.springframework.aop.framework.ProxyFactoryBean">

        <property name="proxyInterfaces">
            <list>
                <value>aop.Calculator</value>
            </list>
        </property>
        <property name="target" ref="cal"/>
        <property name="interceptorNames">
            <list>
                <value>logPrintAroundAdvice</value>
            </list>
        </property>


    </bean>


</beans>